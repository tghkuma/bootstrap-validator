{
  "name": "bootstrap-validator",
  "version": "0.8.5",
  "description": "Bootstrap形式のフォーム値のValidateを行う。",
  "keywords": [
    "Bootstrap",
    "validate",
    "Validation"
  ],
  "main": "js/bootstrap-validator.js",
  "module": "js/bootstrap-validator.esm.js",
  "exports": {
    ".": {
      "import": "./src/index.js"
    }
  },
  "files": ["js"],
  "private": true,
  "directories": {
    "example": "examples"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "jscpd": "^3.3.26",
    "jsdoc": "^3.6.7",
    "standard": "^16.0.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "scripts": {
    "test": "eslint js/*.js",
    "jsdoc": "jsdoc -r src -d jsdoc/",
    "jscpd": "jscpd ./src/**.js",
    "standard": "standard ./src/**.js",
    "standard:fix": "standard --fix ./js/bootstrap-validator.js",
    "super-linter": "docker run -e RUN_LOCAL=true -v `pwd`:/tmp/lint github/super-linter",
    "webpack": "webpack"
  },
  "standard": {
    "ignore": [
      "webpack.config.js",
      "/js/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tghkuma/bootstrap-validator.git"
  },
  "author": {
    "name": "Team-Grasshopper Co., Ltd.",
    "url": "https://team-grasshopper.info/"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tghkuma/bootstrap-validator/issues"
  },
  "homepage": "https://tghkuma.github.io/bootstrap-validator/"
}
